{
  "name": "amp/correlate-php-laravel",
  "description": "Middleware to handle incoming correlation id header for Laravel and Lumen",
  "keywords": [
  	"microservice",
  	"uuid",
  	"dispatcher",
  	"log",
  	"correlate",
  	"laravel",
  	"lumen",
  	"psr-7",
  	"slim"
  ],
  "type": "library",
  "homepage": "https://github.com/sebabentancurt/correlate-php-core",
  "license": "MIT",
  "authors": [
    {
      "name": "Sebastian Bentancurt",
      "email": "sebabentancurt@antel.net.uy"
    }
  ],
	"require": {
    "illuminate/http": "^5.4",
    "psr/log": "1.*",
    "monolog/monolog": "1.*",
    "webpatser/laravel-uuid": "^3.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "^5.0",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "^2.8"
  },
  "autoload": {
    "psr-4": {
      "Amp\\Correlate\\Laravel\\": "src/Laravel",
      "Amp\\Correlate\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Amp\\Correlate\\Laravel\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
